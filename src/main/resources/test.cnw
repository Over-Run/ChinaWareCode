var a <- 1;//赋予动态值a 数值1
val b <- 2;//赋予固定值b 数值1
global c <- (a+b)-(1+1); //创建全局固定变量c 并传递a+b的值
global c -> a,b; //固定变量c传递给a和b
b -> var d,e;
var f <= 1 <> 2;//判断方式为 <=小于等于 => 大于等于 ==等于等于 <>不等于 &&和 || 或

if f {
    var g <- a;
    var h <- a;
}

switch f {
    true : {
        var tmp <- 1;
    }
    false : {
        var tmp <- 1;
    }
}

val k <- "a", "b", "c", "d";

for i <- k {
    var h <- .i;//附加.表示叠加数组
}//这个for语句等效于var h <- k;

var t1 <- 0;
var t2 <- 10;
for t1 -> t2 {
    var j <- .it;
}//从1到10的数组